{
  "name": "extract-release-notes",
  "version": "0.0.0",
  "private": true,
  "description": "A GitHub Action to extract release notes from pull requests",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx ncc build ./src/main.ts",
    "ci": "npm run lint && npm test",
    "format": "prettier --write **/*.ts **/*.md **/*.yaml **/*.yml",
    "lint": "prettier --check **/*.ts **/*.md **/*.yaml **/*.yml",
    "start": "npx ncc run ./src/main.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lee-dohm/extract-release-notes.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "@lee-dohm",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@zeit/ncc": "^0.20.5",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.9.5"
  }
}
